extern putchard(c);
fun printstar(n) {
    for i = 1, i < n, 1.0 {
       putchard(42); # ascii 42 is '*'
       putchard(45); # ascii 45 is '-'
    }
}

fun hello() {
  putchard(72);
  putchard(69);
  putchard(76);
  putchard(76);
  putchard(79);
}

